  <div class="flex flex-row justify-between mb-1 mt-8 pt-28">
    <div class="btn-group w-96 ml-4" role="group" aria-label="Basic example">
      <!-- Active	Locked User	Expiry User	Custom 									 -->

      <button type="button" class="btn btn-primary">Active</button>
      <button type="button" class="btn btn-primary">Locked User</button>
      <button type="button" class="btn btn-primary">Expiry User</button>
    </div>
    <form class="d-flex mr-4">
      <input class="form-control me-2" type="search" placeholder="Search" aria-label="Search">
      <button class="btn btn-outline-success btn-secondary text-black" type="submit">Search</button>
    </form>
  </div>
  <div class="mx-3">
  <table class="table table-light">
    <thead>
      <tr>
        	<!-- Login Id	Password							Validity	Active State	Lock State	Action 	Action -->

        <th scope="col">Name</th>
        <th scope="col">Login Id</th>
        <th scope="col">Password</th>
        <th scope="col">Expires In</th>
        <th scope="col">Validity</th>
        <th scope="col">Active State</th>
        <th scope="col">Lock State</th>
        <th scope="col">Action</th>
        <th scope="col">Action </th>
      </tr>
    </thead>
    <tbody>
      <% for(var i=0;i<result.length;i++) { %>

      <tr>
        <% const oneDay = 24 * 60 * 60 * 1000; %>
          
          <% const firstDate = result[i].validityDate%>
          <% const secondDate = new Date() %>
          
         <% let diffDays = Math.round(Math.abs((firstDate - secondDate) / oneDay));%>
        <td scope="col"><%= result[i].firstName+" "+result[i].middleName+" "+result[i].lastName %></td>
        <td scope="col"><%= result[i].loginId %></td>
        <td scope="col"><%= result[i].loginPassword %></td>
        <td scope="col" <% if(firstDate - secondDate <= 15){ %>
                        class="text-red-500" 
                      <% }  else{%>
                        
                      <% } %>
        >
        <% if(firstDate - secondDate<=0){ %>
          <% diffDays= "Expired" %>
        <% }  else{%>
         <% diffDays= diffDays +" days left" %>
        <% } %>
        <%= diffDays %></td>
        <td scope="col"><%= firstDate.toISOString().split('T')[0] %></td>
        <td scope="col"><% if(result[i].activeState){ %>
                         Active 
                        <% }  else{%>
                          Inactive
                        <% } %>
        </td>
        <td scope="col"><% if(firstDate - secondDate<=0){ %>
                        Locked 
                      <% }  else{%>
                        Not Locked
                      <% } %>
        </td>
        <td scope="col" class="text-green-700">Update</td>
        <td scope="col" class="text-blue-500">Delete</td>
      </tr>

    <% } %>
      
    </tbody>
  </table>
</div>
      
          <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.10.2/dist/umd/popper.min.js" integrity="sha384-7+zCNj/IqJ95wo16oMtfsKbZ9ccEh31eOz1HGyDuCQ6wgnyJNSYdrPa03rtR1zdB" crossorigin="anonymous"></script>
          <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.min.js" integrity="sha384-QJHtvGhmr9XOIpI6YVutG+2QOK9T+ZnN4kzFN1RtK3zEFEIsxhlmWl5/YESvpZ13" crossorigin="anonymous"></script>

</body>
